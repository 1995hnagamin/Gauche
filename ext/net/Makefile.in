srcdir       = @srcdir@
top_builddir = @top_builddir@
top_srcdir   = @top_srcdir@

SCM_CATEGORY = gauche

LIBFILES = libnet.la
SCMFILES = net.scm

OBJECTS = net_head.lo net.lo addr.lo netdb.lo netlib.lo net_tail.lo

GENERATED = Makefile netlib.c net_head.c net_tail.c

all : $(LIBFILES)

libnet.la : $(OBJECTS) 
	$(MODLINK)  $(OBJECTS) $(EXT_LIBGAUCHE)

netlib.c : netlib.stub

net_head.c net_tail.c :
	$(GAUCHE_CONFIG) --fixup-extension net

install : install-std

include ../Makefile.ext

