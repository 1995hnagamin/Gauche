srcdir       = @srcdir@
top_builddir = @top_builddir@
top_srcdir   = @top_srcdir@

SCM_CATEGORY = math

LIBFILES = mt-random.la
SCMFILES = mt-random.scm

OBJECTS = mt-random_head.lo mt-random.lo mt-lib.lo mt-random_tail.lo

GENERATED = Makefile mt-lib.c mt-random_head.c mt-random_tail.c

EXTRA_INCLUDES = -I$(srcdir)/../uvector

all : $(LIBFILES)

mt-random.la : $(OBJECTS) 
	$(MODLINK) $(OBJECTS) @EXT_UVECTOR@ $(EXT_LIBGAUCHE) 

mt-lib.c : mt-lib.stub

mt-random_head.c mt-random_tail.c :
	$(GAUCHE_CONFIG) --fixup-extension mt-random mtrandom

install : install-std

include ../Makefile.ext

