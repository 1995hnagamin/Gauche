#

prefix = @prefix@
exec_prefix = @exec_prefix@
SCM_INSTALL_DIR = @libdir@/gauche/@GAUCHE_VERSION@/lib

SCMS = srfi-0.scm srfi-1.scm srfi-2.scm test.scm

INSTALL=@INSTALL@

all :

clean :
	rm -f core *~ vm/*~

distclean : clean
	rm -f Makefile

realclean : clean
	rm -f Makefile

install : all
	for f in $(SCMS); do \
	  $(INSTALL) -m 444 $$f $(SCM_INSTALL_DIR); \
	done
